  },
  "dependencies": {},
  "devDependencies": {
    "@types/acorn": "^4.0.3",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.22.0",
    "babili": "^0.0.10",
